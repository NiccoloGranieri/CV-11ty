<!doctype html>
<html lang="en-US">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" 
    content="width=device-width, initial-scale=1">
  <style>
    {%- include "styles.css" -%}
  </style>
  <title>
    Resume of 
    {{ resume.basics.name }}, 
    {{ resume.basics.label }}
  </title>
</head>
<body>
  {% 
    include "components/header.liquid", 
    basics: resume.basics
  %}
  <main>
    {% comment %}
    {% 
      include "components/summary.liquid", 
      summary: resume.basics.summary 
    %}
    {% endcomment %}
    {% 
      include "components/experience.liquid", 
      work: resume.work
    %}
    {% 
      include "components/education.liquid", 
      education: resume.education
    %}
    {% 
      include "components/funding.liquid", 
      funding: resume.funding
    %}
    {% 
      include "components/publications.liquid", 
      publications: resume.publications
    %}
    {% 
      include "components/phd-supervision.liquid", 
      phd-supervision: resume.phd-supervision
    %}
    {% 
      include "components/pro-memberships.liquid", 
      pro-memberships: resume.pro-memberships
    %}
  </main>
</body>

</html>